@model Fallah_App.Models.WebMaster
@section title{
    <title>List des admins</title>
}
 @section style
    {
        <style>
    .img-responsive {
  max-width: 100% !important;
  height: 70px !important;
}
    </style>
    }

<div class="breadcomb-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="breadcomb-list">

                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="breadcomb-wp">
                                <div class="breadcomb-icon">
                                    <i class="notika-icon notika-windows"></i>
                                </div>
                                <div class="breadcomb-ctn">
                                    <h2>Admins</h2>
                                    <p>Liste des Admins</p>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Breadcomb area End-->
<!-- Data Table area Start-->
<div class="data-table-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">

                <div class="data-table-list">
                    <div class="table-responsive">
                        <table id="data-table-basic" class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Image</th>
                                    <th>Nom</th>
                                    <th>Prenom</th>
                                    <th>Email</th>                            
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (WebMaster webMaster in ViewBag.listWebMaster)
                                {
                                    <tr>
                                        <td><img class="img-responsive" src="~/imageAdmin/@webMaster.Image" /></td>
                                        <td>@webMaster.Nom</td>
                                        <td>@webMaster.Prenom</td>
                                        <td>@webMaster.Email</td>
                                    </tr>
                                }
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>Image</th>
                                    <th>Nom</th>
                                    <th>Prenom</th>
                                    <th>Email</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>