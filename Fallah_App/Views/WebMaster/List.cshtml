@model Fallah_App.Models.WebMaster
@section title{
    <title>List des admins</title>
}
<div class="breadcomb-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="breadcomb-list">

                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="breadcomb-wp">
                                <div class="breadcomb-icon">
                                    <i class="notika-icon notika-windows"></i>
                                </div>
                                <div class="breadcomb-ctn">
                                    <h2>Liste des Admins</h2>
                                    

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Breadcomb area End-->
<!-- Data Table area Start-->
<div class="data-table-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">

                <div class="data-table-list">
                    <div class="table-responsive">
                        <table id="data-table-basic" class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Nom</th>
                                    <th>Prenom</th>
                                    <th>Email</th>
                            
                                    <th>Action</th>


                                </tr>
                            </thead>
                            <tbody>
                                @foreach (WebMaster webMaster in ViewBag.listWebMaster)
                                {
                                    <tr>
                                        <td>@webMaster.Nom</td>
                                        <td>@webMaster.Prenom</td>
                                        <td>@webMaster.Email</td>
                                   

                                        <td>

                                            <a class="btn btn-success success-icon-notika btn-reco-mg btn-button-mg" asp-controller="WebMaster" asp-action="Modifier" asp-route-id="@webMaster.Id" data-trigger="hover" data-toggle="popover" data-placement="top" data-content="Modifier  @webMaster.Nom" data-original-title="Modifier">
                                                <i class="notika-icon notika-edit"></i>
                                            </a>
                                            <a class="btn btn-deeporange deeporange-icon-notika btn-reco-mg btn-button-mg sa-warning" data-confirm="Supprimer" data-text="Le Admin vas étre supprimer" data-href="/WebMaster/Supprimer/@webMaster.Id" data-trigger="hover" data-toggle="popover" data-placement="top" data-content="supprimer  @webMaster.Nom" data-original-title="Supprimer">
                                                <i class="notika-icon notika-close"></i>
                                            </a>

                                        </td>
                                    </tr>
                                }
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>Nom</th>
                                    <th>Type</th>
                                    <th>Mois de debut</th>
                                    <th>Mois de fin</th>
                                    <th>Action</th>

                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>