@model Fallah_App.Models.Plante
<form method="post">
 <div class="form-element-area">
 <div class="container">
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="form-element-list mg-t-30">
            <div class="cmp-tb-hd">
                <h2>ajouter plante ici</h2>
            </div>
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <div class="form-group ic-cmp-int float-lb floating-lb">
                        <div class="form-ic-cmp">
                            <i class="notika-icon notika-support"></i>
                        </div>
                        <div class="nk-int-st">
                            <input asp-for="Nom" class="form-control">
                            <label asp-for="Nom"></label>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <div class="form-group ic-cmp-int float-lb floating-lb">
                        <div class="form-ic-cmp">
                            <i class="notika-icon notika-mail"></i>
                        </div>
                        <div class="nk-int-st">
                            <input asp-for="type" class="form-control">
                            <label asp-for="type"></label>
                        </div>
                    </div>
                </div>
             </div>
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <div class="form-group ic-cmp-int float-lb floating-lb">
                        <div class="form-ic-cmp">  
                            <i class="notika-icon notika-phone"></i>
                        </div>
                        <div class="nk-int-st">
                            <input asp-for="Debut_Period" class="form-control">
                            <label asp-for="Debut_Period"></label>
                        </div> 
                    </div>
                </div> 
         
       
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <div class="form-group ic-cmp-int float-lb form-elet-mg">
                        <div class="form-ic-cmp">
                            <i class="notika-icon notika-wifi"></i>
                        </div>
                        <div class="nk-int-st">
                            <input asp-for="Fin_Date" class="form-control">
                            <label asp-for="Fin_Date"></label>
                        </div>
                        <div class="form-ic-cmp">
                            <i class="notika-icon notika-cloud"></i>
                        </div>                   
                    </div>
                
            </div>
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="form-group ic-cmp-int float-lb form-elet-mg">                    
                            <div class="nk-int-st">
                                <input type="submit" class="btn btn-success" value="send" />
                            </div>
                      
                        </div>
                    </div>
                </div>
        </div>
    </div>
 
</div>
</div>

</form>