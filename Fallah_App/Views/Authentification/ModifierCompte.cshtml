@model Fallah_App.Models.Agriculteur
@section title
    {
    <title>Modifier mot de passe</title>
}
<form method="post" enctype="multipart/form-data">
    <div class="breadcomb-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="breadcomb-list">
                        <div class="row">
                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                <div class="breadcomb-wp">
                                    <div class="breadcomb-icon">
                                        <i class="notika-icon notika-windows"></i>
                                    </div>
                                    <div class="breadcomb-ctn">
                                        <h2>Modifier les Information</h2>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="form-element-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="form-element-list mg-t-30">
                        <div class="row">
                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                <div class="form-group ic-cmp-int float-lb floating-lb">
                                    <div class="form-ic-cmp">
                                        <i class=""></i>
                                    </div>
                                    <div class="nk-int-st">
                                        <label>Telephone</label>
                                        <input type="text" asp-for="@Model.Telephone" class="form-control input-lg">
                                        <span asp-validation-for="Telephone" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                <div class="form-group ic-cmp-int float-lb floating-lb">
                                    <div class="form-ic-cmp">
                                        <i class=""></i>
                                    </div>
                                    <div class="nk-int-st">
                                        <label>Date_De_Naissance</label>
                                        <input type="date" asp-for="@Model.Date_De_Naissance" class="form-control input-lg">
                                        <span asp-validation-for="Date_De_Naissance" class="text-danger"></span>
                                        <input type="hidden" asp-for="@Model.Email" class="form-control input-lg">
                                        <input type="hidden" asp-for="@Model.Image" class="form-control input-lg">
                                        <input type="hidden" asp-for="@Model.Nom" class="form-control input-lg">
                                        <input type="hidden" asp-for="@Model.Prenom" class="form-control input-lg">
                                        <input type="hidden" asp-for="@Model.Password" class="form-control input-lg">
                                        <input type="hidden" asp-for="@Model.Date_Creation_Compte" class="form-control input-lg">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                <div class="form-group ic-cmp-int float-lb floating-lb">
                                    <div class="form-ic-cmp">
                                        <i class=""></i>
                                    </div>
                                    <div class="nk-int-st">
                                        <label>Login</label>
                                        <input type="text" asp-for="@Model.Login" class="form-control input-lg">
                                        <span asp-validation-for="Login" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                <div class="form-group ic-cmp-int float-lb form-elet-mg">
                                  <div class="col-lg-12 col-md-7 col-sm-7 col-xs-12 dcenter">
                                     <button class="btn btn-success notika-btn-success" type="submit">Valider</button>
                                  </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</form>
